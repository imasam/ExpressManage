<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
    <package name="struts2" extends="struts-default">
        <action name="login" class="loginAction">
            <result name="success" type="redirect">/index.jsp</result>
        </action>

        <action name="register" class="registerAction">
        </action>

        <action name="newExpress" class="newExpressAction">
        </action>

        <action name="queryExpress" class="queryExpressAction">
            <result name="success" type="redirect">/expressInfo.jsp</result>
        </action>

        <action name="updateExpress" class="updateExpressAction">
            <result name="success" type="redirect">/expressInfo.jsp</result>
        </action>
    </package>
</struts>