<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dao.entities.RouteInfo" table="route_info" schema="express">
        <id name="expressNo">
            <column name="express_no" sql-type="varchar(10)" length="10"/>
        </id>
        <property name="info">
            <column name="info" sql-type="tinytext"/>
        </property>
        <property name="time">
            <column name="time" sql-type="datetime"/>
        </property>
    </class>
</hibernate-mapping>